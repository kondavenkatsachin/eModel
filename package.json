  "description": "Repository created in the article REST API Tests with Cypress",
  "main": "index.js",
  "scripts": {
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run --spec '**/*.spec.js'",
    "cypress:run-qa": "./node_modules/.bin/cypress run --spec '**/*.spec.js' --env ENV='qa'",
    "cypress:run-ui-qa-nh": ".\\node_modules\\.bin\\cypress run --spec 'cypress\\integration\\UI_tests\\login_test.spec.js' --env ENV=qa --headed",
    "cypress:run-ui-qa": ".\\node_modules\\.bin\\cypress run --spec 'cypress\\integration\\UI_tests\\*.spec.js' --env ENV=qa",
    "cypress:run-ui-stage-nh": ".\\node_modules\\.bin\\cypress run --spec 'cypress\\integration\\UI_tests\\*.spec.js' --env ENV=stage  --headed",
    "cypress:run-ui-stage": "./node_modules/.bin/cypress run --spec 'cypress/integration/UI_tests/*.spec.js' --env ENV=stage",
    "cypress:run-api": "./node_modules/.bin/cypress run --spec 'cypress/integration/API_tests/*.spec.js'"
  },
